{
  "name": "nimus-cli",
  "version": "1.0.0",
  "description": "Nimus command line interface",
  "main": "dist/main.js",
  "author": "Bruno Fonseca <brunomacf@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel -d dist/ src/",
    "dev": "NODE_PATH=./:./src NODE_ENV=development babel-node ./src/main.js",
    "prod": "babel -d dist/ src/ && NODE_PATH=./:./dist NODE_ENV=production node ./dist/main.js",
    "test": "NODE_PATH=./:./src NODE_ENV=test mocha --reporter list --timeout 30000 --watch --compilers js:babel-core/register --require babel-polyfill --recursive --opts .test.js ./src",
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.8.0",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "@google-cloud/compute": "^0.9.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "cli-spinner": "^0.2.7",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "google-auto-auth": "^0.8.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.19.3",
    "prompt-confirm": "^1.2.0",
    "request": "^2.83.0",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  }
}
